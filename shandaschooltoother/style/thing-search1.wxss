/* asd */

.container {
  position: relative;
}

.cate-btn {
  background: #ff495e;
  bottom: 18rpx;
  right: 18rpx;
  font-size: 20rpx;
  color: #fff;
  padding: 4px 18rpx;
}
.index-cont-search {
  width: 100%;
  font-size: 28rpx;
  position: relative;
  background: #f1f1f1;
}
.index-cont-search icon {
  position: absolute;
  left: 0;
  margin-left: 20rpx;
  margin-top: 16rpx;
  
}
.index-cont-search text {
  margin-left:12rpx;
}

.index-search-box {
  top: 1rpx;
  left: 0rpx;
  right: 0rpx;
  z-index: 999;
  position: fixed;
  background: #fff;
  padding: 18rpx 13rpx;
  border-bottom: 1px solid #eee;
}

.index-search {
  border-bottom: 0;
  background: #fff;
  border-radius: 50rpx;
  overflow: hidden;
  position: relative;
  font-size: 32rpx;
  color: #999;
  box-sizing: border-box;
  height: 64rpx;
  line-height: 64rpx;
  margin: 0 10rpx;
}

.list-right {
  width: 60rpx;
}

.list-right text {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 40rpx;
}

.list-header {
  position: fixed;
  top: 1rpx;
  left: 0;
  right: 0;
}

.no-more {
  text-align: center;
  color: #737373;
  margin: 10px 0;
}

.category-list {
  overflow: hidden;
}

.category-list .list {
  box-sizing: border-box;
  width: 50%;
  float: left;
  margin: 5px 0;
}

.category-list .list:nth-child(2n) {
  border-left: 2px solid #f7f7f7;
  border-bottom: 4px solid #f7f7f7;
}

.category-list .list:nth-child(2n-1) {
  border-right: 2px solid #f7f7f7;
  border-bottom: 4px solid #f7f7f7;
}

.category-list.arrange .list {
  overflow: hidden;
  padding: 15rpx;
  border-bottom: 1rpx solid #f7f7f7;
  width: 100%;
}

.category-list.arrange .list .left {
  width: 35%;
  float: left;
}

.category-list.arrange .list .right {
  width: 65%;
  float: left;
}

.category-list.arrange .list .left .img image {
  width: 220rpx;
  height: 220rpx;
}
.list-msg2{
  width: 220rpx;
  
}
.select_box{
  border-radius: 10px;

}
.select_one {
  height: 60rpx;
  line-height: 60rpx;
  margin: 10px 0;
}

.xiaoxiangshop-notcont {
  margin: 130rpx 100rpx;
}
.xiaoxiangshop-notcont .cont {
  display: block;
  text-align: center;
  font-size: 30rpx;
  color: #999;
  margin-top: 10rpx;
}
.nothing{
  text-align: center;


  margin-top: 15px;
  color:#D1D1D1;
}





/* 猜您喜欢 */

.title-hrbg {
  position: relative;
  z-index: 1;
  height: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
  color: #888;
  text-align: center;
  margin-top: 30rpx;
}

.title-hrbg .cont {
  background: #f7f7f7;
  padding: 1rem 0.6rem;
  font-size: 28rpx;
  z-index: 10;
}

.title-hrbg .cont .icon-huo {
  padding-right: 10rpx;
}

.title-hrbg .hr {
  background: #ddd;
  height: 1rpx;
  border: 0;
  position: absolute;
  left: 10%;
  right: 10%;
  top: 50%;
  margin-top: 1px;
  z-index: -1;
}


/* nav部分样式开始 */
.nav{
  height: 250rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
  font-size: 22rpx;
  font-weight: normal;
  background-color: #fff;
  color: #444;
  margin-top: 30rpx;
  /* margin-bottom: 30rpx; */
  
}
.nav-list{
  width: 20%;
  height: 125rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.nav-list-img{
  margin-top: 15rpx;
  width: 75rpx;
  height: 75rpx;
}
.nav-list-title {
  width: 100%;
  height: 40rpx;
  text-align: center;
}

.bt-text {
  width: 100%;
  display: flex;
  margin-top: 10px;
  background: #fff;
}
.tabBox {
  white-space: nowrap;
  border-bottom: solid 1px #e6e6e6;
  padding-left: 10px;
}
.tabTtle {
  padding: 10px 15px;
  display: inline-block;
}
.tabTtle.active {
  color: red;
  border-bottom: solid 1px red;
}
.bt-contents {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  margin: 10px 0 0 5px;
}
.conNav {
  width: 50%;
}
.con-item {
  margin: 0 10px 10px 0;
  background: #fff;
  padding: 10px;
}
.itemImg {
  width: 100%;
  height: 134px;
} 
.shopName {
  font-size: 26rpx;
  margin: 5rpx 10rpx;
  width: 100%;
}
.goodsImg{
  text-align: center;
  width: 100%;
   margin: 10px;
}
.del {
  text-decoration: line-through;
  padding-left: 10rpx;
  color: #999;
}
